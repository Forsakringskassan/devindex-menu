{
  "name": "@forsakringskassan/devindex-menu",
  "version": "2.1.4",
  "description": "Menu for local development",
  "keywords": [
    "development",
    "menu"
  ],
  "homepage": "https://github.com/Forsakringskassan/devindex-menu",
  "bugs": "https://github.com/Forsakringskassan/devindex-menu/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/devindex-menu.git"
  },
  "license": "ISC",
  "author": "FK",
  "exports": {
    ".": {
      "types": "./dist/types/menu.d.ts",
      "require": "./dist/cjs/menu.js",
      "import": "./dist/esm/menu.js"
    },
    "./pageobjects": {
      "types": "./dist/types/pageobjects.d.ts",
      "require": "./dist/cjs/pageobjects.js",
      "import": "./dist/esm/pageobjects.js"
    }
  },
  "main": "dist/cjs/menu.js",
  "module": "dist/esm/menu.js",
  "types": "dist/types/menu.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "tsc",
    "build": "node ./scripts/generate.mjs",
    "postbuild": "npm run types:main && npm run types:pageobject",
    "types:main": "dts-bundle-generator -o dist/types/menu.d.ts temp/types/menu.d.ts",
    "types:pageobject": "dts-bundle-generator -o dist/types/pageobjects.d.ts temp/types/pageobjects.d.ts",
    "eslint": "eslint --cache .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "npm run prettier:check && npm run eslint",
    "start": "node ./scripts/serve.mjs"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-config"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/preset-env": "7.29.0",
    "@fkui/tsconfig": "6.36.0",
    "@forsakringskassan/apimock-express": "2.13.0",
    "@forsakringskassan/commitlint-config": "3.3.3",
    "@forsakringskassan/eslint-config": "13.3.2",
    "@forsakringskassan/eslint-config-cli": "13.3.2",
    "@forsakringskassan/eslint-config-cypress": "13.3.4",
    "@forsakringskassan/eslint-config-jest": "13.3.4",
    "@forsakringskassan/eslint-config-typescript": "13.3.0",
    "@forsakringskassan/eslint-config-typescript-typeinfo": "13.3.0",
    "@forsakringskassan/prettier-config": "3.3.4",
    "@tsconfig/node22": "22.0.5",
    "@types/node": "22.19.10",
    "cypress": "15.10.0",
    "cypress-html-validate": "8.1.0",
    "dts-bundle-generator": "9.5.1",
    "esbuild": "0.27.3",
    "esbuild-sass-plugin": "3.6.0",
    "npm-pkg-lint": "4.3.4",
    "sass": "1.97.3",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">= 22.0.0"
  },
  "dependencies": {
    "@fkui/logic": "6.36.0"
  }
}
